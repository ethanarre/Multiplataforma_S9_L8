class Page1 extends StatelessWidget{
  @override
  Widget build(BuildContext context){
    return Scaffold( 
      appBar: AppBar(
        title: Text('Esta es la p치gina 1'),
      ),
      body: SafeArea(
        child: Column(
          children: [
            ElevatedButton(
              onPressed: () {
                Navigator.push(context, MaterialPageRoute(builder: (context) => Page2()));
              }, 
              child: Text("Ir a la siguiente p치gina")
            )
          ],
        ),
      ),
    );
  }
}

class Page2 extends StatelessWidget{
  @override
  Widget build(BuildContext context){
    return Scaffold( 
      appBar: AppBar(
        title: Text('Esta es la p치gina 2'),
      ),
      body: SafeArea(
        child: Column(
          children: [
            ElevatedButton(
              onPressed: () {
                Navigator.pop(context);
              }, 
              child: Text("Volver a la p치gina anterior")
            )
          ],
        ),
      ),
    );
  }
}

